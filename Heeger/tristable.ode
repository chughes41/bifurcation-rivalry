El1 = max(((input+normal(0,variance))^nm - wo*Ivr-wo*Ivf),0) * max((1+wa*Ra1),0)

Er2 = max(((input+normal(0,variance))^nm - wo*Ivl-wo*Ivf),0) * max((1+wa*Ra2),0)

Ef = max(((input+normal(0,variance))^nm - wo*Ivl-wo*Ivr),0) * max((1+wa*Raf),0)

Sm = El1+Er2+Ef

dRl1/dt=(-Rl1+((alpha*El1)/(Sm+Hl1^nm + sigma^nm)))/taus
dHl1/dt = (-Hl1 + wh*Rl1)/tauh

dRr2/dt=(-Rr2+((alpha*Er2)/(Sm+Hr2^nm + sigma^nm)))/taus
dHr2/dt = (-Hr2 + wh*Rr2)/tauh

dRf/dt = (-Rf+((alpha*Ef)/(Sm+Hf^nm + sigma^nm)))/taus
dHf/dt = (-Hf + wh*Rf)/tauh


Eb1 = (Rl1)^n
Sb1 = Eb1
dRb1/dt = (-Rb1 + Eb1/(Sb1 + Hb1^n +sigma^n))/taus
dHb1/dt = (-Hb1 + wh*Rb1)/tauh

Eb2 = (Rr2)^n
Sb2 = Eb2
dRb2/dt = (-Rb2 + (Eb2)/(Sb2 + Hb2^n +sigma^n))/taus
dHb2/dt = (-Hb2 + wh*Rb2)/tauh

Ebf = (Rf)^n
Sbf = Ebf
dRbf/dt = (-Rbf + (Ebf)/(Sbf + Hbf^n +sigma^n))/taus
dHbf/dt = (-Hbf + wh*Rbf)/tauh

Ea1 = sign(Rb1-Rb2-Rbf)*(abs(Rb1-Rb2-Rbf))^n
Ea2 = sign(Rb2-Rb1-Rbf)*(abs(Rb2-Rb1-Rbf))^n
Eaf = sign(Rbf-Rb1-Rb2)*(abs(Rbf-Rb1-Rb2))^n

#aux Ea1aux = sign(Rb1-Rb2)*(abs(Rb1-Rb2))^n
#aux Ea2aux = sign(Rb2-Rb1)*(abs(Rb2-Rb1))^n
#aux Eafaux = sign(Rbf-Rb1-Rb2)*(abs(Rbf-Rb1-Rb2))^n

#aux SaS = (abs(Rb1-Rb2))^n + (abs(Rb2-Rb1))^n + sigmaA^n
#aux Ra1Slope = (-Ra1 + Ea1/(Sa + sigmaA^n))/taua
#aux Ra2Slope = (-Ra2 + Ea2/(Sa + sigmaA^n))/taua
#aux fireRate1 = Ea1/( (abs(Rb1-Rb2))^n + (abs(Rb2-Rb1))^n + sigmaA^n)
#aux fireRate2 = Ea2/( (abs(Rb1-Rb2))^n + (abs(Rb2-Rb1))^n + sigmaA^n)
#Sa = (abs(Rb1-Rb2))^n + (abs(Rb2-Rb1))^n + sigmaA^n


fireRate1 = Ea1/( (abs(Rb1-Rb2-Rbf))^n + (abs(Rb2-Rb1-Rbf))^n + (abs(Rbf-Rb1-Rb2))^n + sigmaA^n)
fireRate2 = Ea2/( (abs(Rb1-Rb2-Rbf))^n + (abs(Rb2-Rb1-Rbf))^n + (abs(Rbf-Rb1-Rb2))^n + sigmaA^n)
fireRate3 = Eaf/( (abs(Rb1-Rb2-Rbf))^n + (abs(Rb2-Rb1-Rbf))^n + (abs(Rbf-Rb1-Rb2))^n + sigmaA^n)

dRa1/dt = (-Ra1 + fireRate1)/taua
dRa2/dt = (-Ra2 + fireRate2)/taua
dRaf/dt = (-Raf + fireRate3)/taua

